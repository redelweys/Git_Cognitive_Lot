								<Expression value="(($NullToZero({Размер_обеспечения_заявки})) &lt;= ($NullToZero({НМЦД})))"/>
								<FailtureText value="Размер обеспечения заявки не должен превышать НМЦ."/>
							</VerifyExpression>
						</Actions>
					</ConditionalAction>
					<ConditionalAction>
						<Conditions>
							<Or>
								<WhenFieldsEquals path="|Document|Обеспечение_исполнения_договора" tag="{current}" value="1" />
							</Or>
						</Conditions>
						<Actions>
							<VerifyExpression>
								<Variables>
									<Variable name="Размер_обеспечения_договора" source="{current}" path="|Document|Размер_обеспечения_договора"  />
									<Variable name="НМЦД" source="{current}" path="|Document|НМЦ_с_НДС"  />
								</Variables>
								<Expression value="(($NullToZero({Размер_обеспечения_договора})) &lt;= ($NullToZero({НМЦД})))"/>
								<FailtureText value="Размер обеспечения договора не должен превышать НМЦ."/>
							</VerifyExpression>
						</Actions>
					</ConditionalAction>
					<ConditionalAction>
						<Conditions>
							<Or>
								<WhenFieldsEquals path="|Document|Обеспечение_возврата_аванса" tag="{current}" value="1" />
							</Or>
						</Conditions>
						<Actions>
							<VerifyExpression>
								<Variables>
									<Variable name="Размер_обеспечения_возврата_аванса" source="{current}" path="|Document|Размер_обеспечения_возврата_аванса"  />
									<Variable name="НМЦД" source="{current}" path="|Document|НМЦ_с_НДС"  />
								</Variables>
								<Expression value="(($NullToZero({Размер_обеспечения_возврата_аванса})) &lt;= ($NullToZero({НМЦД})))"/>
								<FailtureText value="Размер обеспечения возврата аванса не должен превышать НМЦ."/>
							</VerifyExpression>
						</Actions>
					</ConditionalAction>
					<ConditionalAction>
						<Conditions>
							<Or>
								<WhenFieldsEquals path="|Document|Обеспечение_гарантийных_обязательств" tag="{current}" value="1" />
							</Or>
						</Conditions>
						<Actions>
							<VerifyExpression>
								<Variables>
									<Variable name="Размер_обеспечения_гарантийных_обязательств" source="{current}" path="|Document|Размер_обеспечения_гарантийных_обязательств"  />
									<Variable name="НМЦД" source="{current}" path="|Document|НМЦ_с_НДС"  />
								</Variables>
								<Expression value="(($NullToZero({Размер_обеспечения_гарантийных_обязательств})) &lt;= ($NullToZero({НМЦД})))"/>
								<FailtureText value="Размер обеспечения гарантийных обязательств не должен превышать НМЦ."/>
							</VerifyExpression>
						</Actions>
					</ConditionalAction>
					<ConditionalAction>
						<Conditions>
							<Or>
								<WhenFieldsEquals path="|Document|Сведения_о_цене_код" tag="{current}" value="3" />
								<WhenFieldsEquals path="|Document|Сбербанк_АСТ|Дополнительная_информация|Детализация_предложения|Детализация_предложения_наименование" tag="{current}" value="По позициям лота" />
								<WhenFieldsEquals path="|Document|Сбербанк_АСТ|Дополнительная_информация|Детализация_предложения|Детализация_предложения_наименование" tag="{current}" value="По части позиций лота" />
							</Or>
						</Conditions>
						<Actions>
							<TableFieldOperations sourceTag="{current}" targetTag="Позиции плана закупок 223ФЗ" targetTable="|Document|Позиции">
								<SourceTable tableName="|Document|Позиции">                                                                                                                      
									<Column name="NUM" />
									<Column name="Цена_за_единицу_ТРУ" />
									<Column name="Дополнительные_сведения" />
								</SourceTable>  
								<Action column="Цена_за_единицу_ТРУ" formula="iif({NUM}={sourceTableColumn=NUM}, $NullToZero({sourceTableColumn=Цена_за_единицу_ТРУ}), $NullToZero({Цена_за_единицу_ТРУ}))" />
								<Action column="Дополнительные_сведения" formula="iif({NUM}={sourceTableColumn=NUM}, {sourceTableColumn=Дополнительные_сведения}, {Дополнительные_сведения})" />
							</TableFieldOperations>
						</Actions>
					</ConditionalAction>
					<!-- Обеспечение заявок -->
					<!-- Начало -->
					<ConditionalAction>
						<Conditions>
							<And>
								<WhenFieldsEquals path="|Document|С_учетом_НДС" tag="{current}" value="1" />
								<WhenFieldsEquals path="|Document|Редактируется_из_плагина" tag="{current}" value="1" />
								<!-- <WhenFormula param-someParam1="{current}|Document|НМЦ_с_НДС" param-someParam2="{oldCurrent}|Document|НМЦ_с_НДС" formula="{someParam1}!={someParam2}"/> --> 
							</And>
						</Conditions>
						<Actions>
							<CalculateExpression targetFlow="{current}" targetField="|Document|Размер_обеспечения_заявки">
								<Variables>                          
									<Variable name="Процент" source="{current}" path="|Document|Размер_обеспечения_заявки_в_процентах"/>
									<Variable name="НМЦ" source="{current}" path="|Document|НМЦ_с_НДС"/>   		
								</Variables>
								<Expression value="$NullToZero({НМЦ})*$NullToZero({Процент})/100"/>
							</CalculateExpression>					
							<CalculateExpression targetFlow="{current}" targetField="|Document|Размер_обеспечения_возврата_аванса">
								<Variables>                          
									<Variable name="Процент" source="{current}" path="|Document|Размер_обеспечения_возврата_аванса_в_процентах"/>
									<Variable name="НМЦ" source="{current}" path="|Document|НМЦ_с_НДС"/>   		
								</Variables>
								<Expression value="$NullToZero({НМЦ})*$NullToZero({Процент})/100"/>
							</CalculateExpression>
							<CalculateExpression targetFlow="{current}" targetField="|Document|Размер_обеспечения_договора">
								<Variables>                          
									<Variable name="Процент" source="{current}" path="|Document|Размер_обеспечения_договора_в_процентах"/>
									<Variable name="НМЦ" source="{current}" path="|Document|НМЦ_с_НДС"/>   		
								</Variables>
								<Expression value="$NullToZero({НМЦ})*$NullToZero({Процент})/100"/>
							</CalculateExpression>
							<CalculateExpression targetFlow="{current}" targetField="|Document|Размер_обеспечения_гарантийных_обязательств">
								<Variables>                          
									<Variable name="Процент" source="{current}" path="|Document|Размер_обеспечения_гарантийных_обязательств_в_процентах"/>
									<Variable name="НМЦ" source="{current}" path="|Document|НМЦ_с_НДС"/>   		
								</Variables>
								<Expression value="$NullToZero({НМЦ})*$NullToZero({Процент})/100"/>
							</CalculateExpression>
						</Actions>
					</ConditionalAction>
					<ConditionalAction>
						<Conditions>
							<And>
								<WhenFieldsNotEquals path="|Document|С_учетом_НДС" tag="{current}" value="1" />
								<WhenFieldsEquals path="|Document|Редактируется_из_плагина" tag="{current}" value="1" />
								<!-- <WhenFormula param-someParam1="{current}|Document|НМЦ_без_НДС" param-someParam2="{oldCurrent}|Document|НМЦ_без_НДС" formula="{someParam1}!={someParam2}"/>  -->
							</And>
						</Conditions>
						<Actions>
							<CalculateExpression targetFlow="{current}" targetField="|Document|Размер_обеспечения_заявки">
								<Variables>                          
									<Variable name="Процент" source="{current}" path="|Document|Размер_обеспечения_заявки_в_процентах"/>
									<Variable name="НМЦ" source="{current}" path="|Document|НМЦ_без_НДС"/>   		
								</Variables>
								<Expression value="$NullToZero({НМЦ})*$NullToZero({Процент})/100"/>
							</CalculateExpression>					
							<CalculateExpression targetFlow="{current}" targetField="|Document|Размер_обеспечения_возврата_аванса">
								<Variables>                          
									<Variable name="Процент" source="{current}" path="|Document|Размер_обеспечения_возврата_аванса_в_процентах"/>
									<Variable name="НМЦ" source="{current}" path="|Document|НМЦ_без_НДС"/>   		
								</Variables>
								<Expression value="$NullToZero({НМЦ})*$NullToZero({Процент})/100"/>
							</CalculateExpression>
							<CalculateExpression targetFlow="{current}" targetField="|Document|Размер_обеспечения_договора">
								<Variables>                          
									<Variable name="Процент" source="{current}" path="|Document|Размер_обеспечения_договора_в_процентах"/>
									<Variable name="НМЦ" source="{current}" path="|Document|НМЦ_без_НДС"/>   		
								</Variables>
								<Expression value="$NullToZero({НМЦ})*$NullToZero({Процент})/100"/>
							</CalculateExpression>
							<CalculateExpression targetFlow="{current}" targetField="|Document|Размер_обеспечения_гарантийных_обязательств">
								<Variables>                          
									<Variable name="Процент" source="{current}" path="|Document|Размер_обеспечения_гарантийных_обязательств_в_процентах"/>
									<Variable name="НМЦ" source="{current}" path="|Document|НМЦ_без_НДС"/>   		
								</Variables>
								<Expression value="$NullToZero({НМЦ})*$NullToZero({Процент})/100"/>
							</CalculateExpression>
						</Actions>
					</ConditionalAction>
					<!-- Конец -->
				</Actions>
			</ConditionalActions>
		</OnRegcardEdited>
		<OnLinkAdded transaction="split">
			<Aggregation>
				<Select>
					<Select searchString="document[doc_RegCard/rc_FlowName = 'Позиции плана закупок 223ФЗ' and xn-in(doc_Id, {currentDocLinkList})]" tag="Позиции плана закупок 223ФЗ" />
				</Select>
				<Actions>
					<SetField targetFlow="Позиции плана закупок 223ФЗ" targetField="|Document|Статус" value="Включена в заявку на размещение на закупку" />	
					<Sum sourceFlow="Позиции плана закупок 223ФЗ" targetFlow="{current}" sourceField="|Document|НМЦ_с_НДС" targetField="|Document|НМЦ_с_НДС" />
				</Actions>
			</Aggregation>
		</OnLinkAdded>
		<OnLinkDeleted transaction="split">
			<Aggregation>
				<Select>
					<Select searchString="document[doc_RegCard/rc_FlowName = 'Позиции плана закупок 223ФЗ' and xn-in(doc_Id, {currentDocLinkList})]" tag="Позиции плана закупок 223ФЗ" />
				</Select>
				<Actions>
					<Sum sourceFlow="Позиции плана закупок 223ФЗ" targetFlow="{current}" sourceField="|Document|НМЦ_с_НДС" targetField="|Document|НМЦ_с_НДС" />
				</Actions>
			</Aggregation>
		</OnLinkDeleted>
		<OnDocumentDeleted>
			<Aggregation>
				<Select>
					<Select searchString="document[doc_RegCard/rc_FlowName = 'Извещения о закупочной процедуре 223ФЗ' and xn-in(doc_Id, {currentDocLinkList})]" tag="Извещение" />
					<Select searchString="document[doc_RegCard/rc_FlowName = 'Заявка на размещение закупки 223ФЗ' and (doc_Id = document[doc_Links/doc_Link/link_Document={Извещение}/doc_Id] or doc_Id = document[doc_Links/doc_Link/link_Owner={Извещение}/doc_Id])]" tag="Лоты" />
				</Select>
				<Actions>
					<Sum sourceFlow="Лоты" targetFlow="Извещение" sourceField="|Document|НМЦ_с_НДС" targetField="|Document|НМЦ_с_НДС" />
					<Sum sourceFlow="Лоты" targetFlow="Извещение" sourceField="|Document|НМЦД_в_рублях" targetField="|Document|НМЦД_в_рублях" />
					<Min sourceFlow="Лоты" targetFlow="Извещение" sourceField="|Document|Плановая_дата_размещения" targetField="|Document|Порядок_проведения_процедуры|Плановая_дата_размещения" />
					
				</Actions>
			</Aggregation>
		</OnDocumentDeleted>
		<OnRouteStarted>
			<ConditionalActions>
				<ConditionalAction>
					<Conditions>
						<Condition name="RouteKey" value="15" />
					</Conditions>				
				</ConditionalAction>
			</ConditionalActions>
		</OnRouteStarted>
		<OnAttachmentCreated transaction="single">
			<Aggregation>
				<Select>
				</Select>
				<Actions>
					<SetField targetFlow="{current}" targetField="|Document|Требуется_форма" value="1" />
				</Actions>
			</Aggregation>
		</OnAttachmentCreated>
</Hooks>